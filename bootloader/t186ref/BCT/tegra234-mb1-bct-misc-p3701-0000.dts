/dts-v1/;

#include "tegrabl_carveout_id.h"
#include "tegra234-mb1-bct-misc-common.dtsi"
#include "tegra234-mb1-bct-carveout-l4t.dtsi"

/ {
    misc {
        disable_sc7 = <0>;
        enable_tsec = <0>;
        enable_4pin_ram_code = <1>;
        soctherm {
            max_chip_limit = <0x69>;
            min_chip_limit = <0xFFFFFFE4>;
        };
        vmon {
            vdd_soc {
                soc_vmon_config@0 {
                    sku_info = <0x0>;
                    vmin_threshold = <0x17>;
                    vmax_threshold = <0x76>;
                };
            };
            vdd_cpu {
                cpu_vmon_config@0 {
                    sku_info = <0x0>;
                    vmin_threshold = <0x14>;
                    vmax_threshold = <0x7b>;
                };
            };
        };
    };
};
